<script type="text/javascript">
$(function(){

	zdbf_pic2("#zttj2","#zttj2 a");
	//zdbf_pic2(".leftGdPic",".leftGdPic a");
	zdbf_pic2("#leftcover","#leftcover a");
	
	
	function zdbf_pic2(zt,zt2){
	var gd_set=0;
	var gd_tol = $(zt2).length;
	var timeId1a = setInterval(function(){autoShow(gd_set);},5000); 
	function autoShow(set){
		$(zt2).eq(set).fadeIn().siblings().hide();
		gd_set++;
		if(gd_set==gd_tol)gd_set=0;
	};
	$(zt).mouseover(function(){
		clearInterval(timeId1a);					  
	}).mouseout(function(){
		beginAutoNav();
	});
	function beginAutoNav()
	{ timeId1a = setInterval(function(){autoShow(gd_set);},5000);};
	}
	
	
})


</script>
